name: dockercompose14629030268975711739
services:
  identityservice:
    build:
      context: C:\Users\Ahmad\repos\QMedia
      dockerfile: src/IdentityService/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      POSTGRES_DB_Name: registrationservice
      POSTGRES_PASSWORD: one_pw
      POSTGRES_USER: one_user
    image: identityservice
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Ahmad\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Ahmad\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  postgres-db:
    container_name: postgres-db
    environment:
      POSTGRES_DB: identities-db
      POSTGRES_PASSWORD: test
      POSTGRES_USER: test
    image: postgres:14.3
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "25432"
        protocol: tcp
    restart: always
networks:
  default:
    name: dockercompose14629030268975711739_default